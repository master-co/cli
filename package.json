{
    "name": "@master/cli",
    "version": "0.0.0",
    "bin": {
        "m": "./bin/run"
    },
    "engines": {
        "node": ">=12.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/master-style/cli.git"
    },
    "files": [
        "/bin",
        "/lib"
    ],
    "license": "MIT",
    "private": false,
    "main": "lib/index.js",
    "oclif": {
        "commands": "./lib/commands",
        "bin": "m",
        "plugins": [
            "@oclif/plugin-help"
        ]
    },
    "scripts": {
        "build": "rimraf lib && npm i && tsc -b",
        "watch": "tsc -b --watch",
        "update:readme": "oclif-dev readme"
    },
    "types": "lib/index.d.ts",
    "devDependencies": {
        "@oclif/dev-cli": "^1.26.8",
        "@types/inquirer": "^8.1.3",
        "@types/listr": "^0.14.4",
        "@types/node": "^10.17.60",
        "globby": "^10.0.2",
        "rimraf": "^3.0.2",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.10"
    },
    "dependencies": {
        "@master/text-template": "^1.0.2",
        "@oclif/command": "^1.8.9",
        "@oclif/config": "^1.18.2",
        "@oclif/plugin-help": "^3.2.14",
        "execa": "^5.1.1",
        "inquirer": "^8.2.0",
        "listr": "^0.14.2",
        "readjson": "^2.2.2",
        "rxjs": "^7.4.0",
        "tslib": "^1.14.1",
        "writejson": "^3.0.0"
    }
}
